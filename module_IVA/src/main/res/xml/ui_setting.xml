<?xml version="1.0" encoding="utf-8"?><PreferenceScreen    xmlns:android="http://schemas.android.com/apk/res/android"    android:key="root_screen"><PreferenceCategory    android:title="界面个性化">    <SwitchPreference        android:defaultValue="true"        android:key="set_statusbar_transparent"        android:title="是否启用状态栏透明化"        />    <ListPreference        android:defaultValue="#ff333333"        android:entries="@array/theme_color_values"        android:entryValues="@array/theme_color_values"        android:key="set_theme_color"        android:title="主题颜色" /></PreferenceCategory>    <PreferenceCategory        android:title="高级界面个性化(谨慎操作)">        <SwitchPreference            android:defaultValue="true"            android:key="set_gaussian_blur"            android:title="是否启用界面IOS模糊化"            />        <EditTextPreference            android:defaultValue="5"            android:dialogTitle="请输入模糊半径(0-9 默认值:5 )"            android:inputType="number"            android:key="set_gaussian_blur_radius"            android:singleLine="true"            android:maxLength="1"            android:title="界面模糊半径设置" />        <EditTextPreference            android:dialogTitle="请输入缩小倍数(0-9 默认值:3)"            android:key="set_gaussian_blur_scale"            android:defaultValue="3"            android:singleLine="true"            android:maxLength="1"            android:inputType="number"            android:title="界面缩放设置" />        <EditTextPreference            android:dialogTitle="请输入透明度(0-9 默认值:5 )"            android:key="set_gaussian_blur_transparency"            android:defaultValue="5"            android:singleLine="true"            android:maxLength="1"            android:inputType="number"            android:title="界面透明设置" />    </PreferenceCategory>    <PreferenceCategory        android:title="语音发音">        <ListPreference            android:defaultValue="OfflineResource.VOICE_DUYY"            android:entries="@array/tts_load_model"            android:entryValues="@array/tts_load_model_value"            android:key="set_tts_load_model"            android:title="发音人设置" />        <EditTextPreference            android:dialogTitle="设置发音音量(0-9 默认值:9 )"            android:key="set_PARAM_SPEED"            android:defaultValue="9"            android:singleLine="true"            android:maxLength="1"            android:inputType="number"            android:title="发音音量" />        <EditTextPreference            android:dialogTitle="设置发音语速(0-9 默认值:5 )"            android:key="set_PARAM_SPEED"            android:defaultValue="5"            android:singleLine="true"            android:maxLength="1"            android:inputType="number"            android:title="发音语速" />        <EditTextPreference            android:dialogTitle="设置发音语调(0-9 默认值:5 )"            android:key="set_PARAM_PITCH"            android:defaultValue="5"            android:singleLine="true"            android:maxLength="1"            android:inputType="number"            android:title="发音语调" />    </PreferenceCategory></PreferenceScreen>